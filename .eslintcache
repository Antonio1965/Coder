ategory\">\r\n            <h2>{category_name.split('-').join(' ')}</h2>\r\n            <ProductsList products={prods} />\r\n        </section>\r\n    )\r\n}\r\n\r\nexport default Category;","C:\\Users\\test\\proyecto-coder\\src\\components\\Home\\index.jsx",["129"],"C:\\Users\\test\\proyecto-coder\\src\\components\\Detail\\ProductDetail.jsx",["130","131"],"import {useState,useContext, useEffect} from 'react';\r\nimport './ProductDetail.css';\r\nimport {Store} from '../../store';\r\nimport {useHistory} from 'react-router-dom';\r\nimport {getFirestore} from '../../db';\r\n\r\n\r\nconst ProductDetail = ({item}) => {\r\n    const history = useHistory();\r\n    const [data, setData] = useContext(Store);\r\n    const [qty, setQty] = useState(1);\r\n    const db = getFirestore();\t\r\n\r\n    const handleClickResta = () => {\t\r\n        if(qty > 1) {\t\r\n            setQty(qty - 1);\t\r\n        }\t\r\n    }\t\r\n\r\n    const onAdd = () => {\r\n        console.log(item);\r\n        setData({\r\n            ...data, \r\n            cantidad: data.cantidad + qty,\r\n            items: [...data.items, {item: item, cantidad: qty}],\r\n            precioTotal: data.precioTotal + (item.price * qty)\r\n        });\r\n        history.push('/cart');\t\r\n    }\r\n\r\n\r\n   \r\n\r\n    return (\r\n        <article className=\"product\">\r\n            <div className=\"foto\">\r\n                 <img src={`/products/${item.img}`} alt=\"\"/>\r\n            </div>\r\n\r\n            <div className=\"info\">\r\n                <h1 className=\"title\">{item.title}</h1>\r\n                {\r\n                    !!item.description && <p className=\"description\">{item.description}</p>\r\n                }\r\n                <p className=\"price\">${item.price}</p>\r\n\r\n                <div className=\"qty\">\t\r\n                    <button \t\r\n                        disabled={qty === 1 ? 'disabled' : null } \t\r\n                        onClick={handleClickResta}\t\r\n        